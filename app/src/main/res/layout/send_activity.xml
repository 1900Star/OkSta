<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <waimai.yunlian.com.okstar.TitelLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </waimai.yunlian.com.okstar.TitelLayout>
    <EditText android:id="@+id/edt_pwd" android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:hint="请输入密码"/>
    <EditText android:id="@+id/edt_name" android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:hint="请输入姓名"/>
    <Button android:id="@+id/btn" android:layout_width="match_parent"
            android:layout_height="wrap_content" android:text="第二个"/>

</LinearLayout>